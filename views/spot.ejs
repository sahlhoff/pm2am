function initialize() {
  var fenway = new google.maps.LatLng('<%=lat%>','<%=lng%>');

  var panoramaOptions = {
    position: fenway,
    pov: {
      heading: 34,
      pitch: 10
    }
  };
  var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);

}

google.maps.event.addDomListener(window, 'load', initialize);

</script>
